# Supabase Configuration for Farm Aris
# Copy this file to .env and fill in your actual values

# Public Environment Variables (safe to expose to frontend)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Private Environment Variables (server-side only - DO NOT expose to frontend)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_SECRET_KEY=your_secret_key

# Farm Aris Specific Configuration
VITE_APP_NAME=Farm Aris Grand Opening
VITE_APP_LOCATION=Grootfontein, Namibia
VITE_EVENT_DATE=2024-12-15
VITE_CONTACT_PHONE=+264123456789
VITE_CONTACT_EMAIL=info@farmaris.com

# Development Configuration
NODE_ENV=development